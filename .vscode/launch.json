    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Cargo launch",
            "cargo": {
                "args": [
                    "build"
                ],
            },
            "program": "${cargo:program}",
            //"args": ["set_intensity", "--intensity", "100"],
            "args": ["disable", "lower"],
            "stopOnEntry": false,
            "sourceLanguages": [
              "rust"
            ]
        }
    ]
}